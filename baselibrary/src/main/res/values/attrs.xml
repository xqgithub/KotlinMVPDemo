<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="textviewhaha">
        <attr name="textview1" format="integer" />
        <attr name="textview2" format="integer" />
    </declare-styleable>


    <string-array name="loading_anim">
        <item>@mipmap/progress_circle_animation1</item>
        <item>@mipmap/progress_circle_animation2</item>
        <item>@mipmap/progress_circle_animation3</item>
        <item>@mipmap/progress_circle_animation4</item>
        <item>@mipmap/progress_circle_animation5</item>
        <item>@mipmap/progress_circle_animation6</item>
        <item>@mipmap/progress_circle_animation7</item>
        <item>@mipmap/progress_circle_animation8</item>
        <item>@mipmap/progress_circle_animation9</item>
        <item>@mipmap/progress_circle_animation10</item>
        <item>@mipmap/progress_circle_animation11</item>
        <item>@mipmap/progress_circle_animation12</item>
        <item>@mipmap/progress_circle_animation13</item>
        <item>@mipmap/progress_circle_animation14</item>
        <item>@mipmap/progress_circle_animation15</item>
        <item>@mipmap/progress_circle_animation16</item>
        <item>@mipmap/progress_circle_animation17</item>
        <item>@mipmap/progress_circle_animation18</item>
        <item>@mipmap/progress_circle_animation19</item>
        <item>@mipmap/progress_circle_animation20</item>
    </string-array>


    <!-- 自定义 EditTextCustomize -->
    <declare-styleable name="EditTextCustomize">
        <!-- 显示删除按钮，true 显示，false 不显示（默认） -->
        <attr name="isShowClearDrawable" format="boolean" />
        <!-- 删除按钮图片资源 -->
        <attr name="clearDrawableRes" format="reference" />
        <!-- 删除按钮图片宽度 -->
        <attr name="clearDrawableWidth" format="dimension" />
        <!-- 删除按钮图片高度 -->
        <attr name="clearDrawableHeight" format="dimension" />
        <!-- 删除按钮图片距离输入框右边距 -->
        <attr name="clearDrawablePaddingRight" format="dimension" />
    </declare-styleable>


</resources>